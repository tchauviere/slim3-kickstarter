    <body>
        <div id="error">
            <div class="error">
                <div class="error-500">
                    <h1>{{ '500_title'|trans }}</h1>
                </div>
                <h2>{{ '500_subtitle'|trans }}</h2>
                <p>{{ '500_msg'|trans }}</p>
                <p>{{ '500_code'|trans }} : #{{ code }}</p>
                <a href="#" onclick="window.history.go(-1); return false;">{{ 'back'|trans }}</a>
            </div>
        </div>
