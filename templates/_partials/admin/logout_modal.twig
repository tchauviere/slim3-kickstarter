<!-- Logout Modal-->
<div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="logoutModalLabbel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="logoutModalLabbel">{{ 'logout_modal_title'|trans }}</h5>
                <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">Ã—</span>
                </button>
            </div>
            <div class="modal-body">{{ 'logout_msg'|trans }}</div>
            <div class="modal-footer">
                <button class="btn btn-secondary" type="button" data-dismiss="modal">{{ 'cancel_btn'|trans }}</button>
                <a class="btn btn-primary" href="{{ path_for('getLogout') }}">{{ 'logout_btn'|trans }}</a>
            </div>
        </div>
    </div>
</div>